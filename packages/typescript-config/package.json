  },
  "peerDependencies": {
    "typescript": ">=4.7.x",
    "typescript-eslint-language-service": ">=5.x.x"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "typescript-eslint-language-service": {
      "optional": true
    }
