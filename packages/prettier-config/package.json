{
  "name": "@kurone-kito/prettier-config",
  "version": "0.7.3",
  "description": "My Prettier configuration for general projects",
  "keywords": [
    "config",
    "prettier",
    "prettierconfig",
    "prettierrc"
  ],
  "homepage": "https://github.com/kurone-kito/lints-config#readme",
  "bugs": "https://github.com/kurone-kito/lints-config/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/kurone-kito/lints-config.git",
    "directory": "packages/prettier-config"
  },
  "author": "kurone-kito <krone@kit.black> (https://kit.black/)",
  "type": "module",
  "exports": {
    ".": "./.prettierrc.json",
    "./.prettierrc.json": "./.prettierrc.json"
  },
  "main": ".prettierrc.json",
  "files": [
    ".prettierrc.json"
  ],
  "scripts": {
    "build": "js-yaml .prettierrc.yml > .prettierrc.json",
    "clean": "rimraf -g \".prettierrc.json\" \"*.tgz\" \"undefined\"",
    "lint": "yarn run prettier -cu",
    "lint:fix": "yarn run prettier -uw",
    "prepack": "yarn run clean && yarn run build",
    "prettier": "prettier --cache --loglevel=warn \"$@\" \"./**/*\""
  },
  "devDependencies": {
    "js-yaml": "^4.1.0",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1"
  },
  "peerDependencies": {
    "prettier": ">=2.3.x"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.20"
  },
  "publishConfig": {
    "access": "public"
  }
}
