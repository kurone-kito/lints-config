{
  "name": "@kurone-kito/eslint-config-base",
  "version": "0.2.7",
  "description": "My ESLint configuration for general Node.js projects",
  "keywords": [
    "config",
    "eslint",
    "eslintconfig",
    "eslintrc"
  ],
  "homepage": "https://github.com/kurone-kito/lints-config#readme",
