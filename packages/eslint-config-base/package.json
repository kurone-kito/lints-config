    "eslint-plugin-markdown": ">=3.x.x",
    "eslint-plugin-markdownlint": ">=0.5.x",
    "eslint-plugin-n": ">=17.x.x",
    "eslint-plugin-oxlint": ">=0.2.x",
    "eslint-plugin-yaml": ">=0.5.x"
  },
